{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ],
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@eui/deps-base/node_modules/@types"
    ],
    "resolveJsonModule": true,
    "preserveSymlinks": false,
    "paths": {
        "@shared/*": ["src/app/shared/*"],
        "@angular/*": ["node_modules/@angular/*"],
        "@eui/*": ["node_modules/@eui/*"]
    }
  }
}
